{
  "name": "aws-cdk-typescript",
  "version": "1.0.0",
  "description": "AWS CDK Website Infrastructure in TypeScript",
  "bin": {
    "aws-cdk-typescript": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
