    "diff": "npm run build && cdk diff",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.100.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
